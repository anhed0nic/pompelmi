    "src/browser-index.ts",
    "packages/*/src/index.ts"
  ],
  "project": [
    "src/**/*.ts",
    "packages/*/src/**/*.ts",
    "!**/{test,tests}/**",
    "!examples/**",
    "!website/**",
    "!site/**",
    "!scripts/**",
    "!**/*.d.ts"
