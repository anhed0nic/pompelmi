{
  "name": "@pompelmi/engine",
  "version": "0.11.11-dev.6",
  "private": false,
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pompelmi/pompelmi.git",
    "directory": "packages/engine"
  },
  "files": [
    "dist",
    "result.schema.json",
    "README.md"
  ],
  "publishConfig": {
    "access": "public",
    "tag": "dev"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --clean",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.cjs",
    "schema:json": "node --input-type=module -e \"import('./dist/index.mjs').then(m=>console.log(JSON.stringify(m.resultJsonSchema, null, 2)))\" > result.schema.json"
  },
  "dependencies": {
    "@fastify/cors": "^10.0.0",
    "@fastify/multipart": "^9.0.3",
    "fastify": "^5.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.19.10",
    "@vitest/coverage-v8": "2.1.9",
    "tsup": "^8.0.2",
    "tsx": "^4.15.7",
    "typescript": "^5.6.2",
    "vitest": "2.1.9",
    "zod-to-json-schema": "^3.23.5"
  },
  "engines": {
    "node": ">=18"
  }
}
